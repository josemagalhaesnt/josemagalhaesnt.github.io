{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false
    },
    "extends": ["react-app","prettier"],
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "max-len": ["error", {
            "code": 100
        }],
        "prefer-promise-reject-errors": ["off"],
        "react/jsx-filename-extension": ["off"],
        "react/prop-types": ["warn"],
        "no-return-assign": ["off"]
    }
}
